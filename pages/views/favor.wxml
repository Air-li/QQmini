<view style="height:200rpx;width:100%"></view>
<view wx:for="{{myFavor}}">
  <view>{{item.name}}</view>
  <view>{{item.restaurant}}</view>
  <view>{{item.locate}}</view>
  <view>
    <image mode="widthFix" style="width:100rpx" src="{{item.src}}"></image>
  </view>
  <view>{{item.date}}</view>
</view>